<!--
  Have to append the following rules to temporarily suppress a handful of violations in legacy code.
  -->
<suppressions>
    <suppress checks="JavadocPackage" files=".*.java" />
    <suppress checks="JavadocType" files=".*.java" />
    <suppress checks="JavadocMethod" files=".*.java" />
    <suppress checks="JavadocVariable" files=".*.java" />
    <suppress checks="JavadocPackage" files=".*.java" />
    <suppress checks="JavadocStyleCheck" files=".*.java" />
    <suppress checks="checkFirstSentence" files=".*.java" />

    <suppress checks="ParameterNumber" files=".*Command.java" />
    <suppress checks="ParameterNumber" files=".*CommandV\d.java" />
    <suppress checks="ParameterNumber" files=".*Test.java" />
    <suppress checks="ParameterNumber" files="Disclose\.java" />
    <suppress checks="ParameterNumber" files="PostalInfo\.java" />

    <suppress checks="OperatorWrapCheck" files=".*Test.java" />

    <suppress checks="HideUtilityClassConstructorCheck" files="TmchXmlParser\.java" />

    <!--
     Suppress the checks for illegal pattern, e.g.
         + `e.printStackTrace()`
         + `System.out.println()`
      -->
    <suppress checks="RegexpCheck" files=".*Test\.java" />
    <suppress checks="RegexpCheck" files=".*Demo.java" />
    <suppress checks="RegexpCheck" files="SessionManagerFactory\.java" />

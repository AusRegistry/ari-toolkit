    <!-- Variables 'cmdElement', 'command' and 'extension' shall be defined as private with accessor methods -->
    <suppress checks="VisibilityModifierCheck" files="Command\.java" />
    <!-- Variables 'maintLogger', 'userLogger' and 'debugLogger' shall be defined as private with accessor methods -->
    <suppress checks="VisibilityModifierCheck" files="ReceiveSE\.java" />
    <!-- Variable `resultArray` shall be defined as private with accessor methods -->
    <suppress checks="VisibilityModifierCheck" files="Response\.java" />
    <!-- Variables `xmlWriter` and `userLogger` shall be defined as private with accessor methods -->
    <suppress checks="VisibilityModifierCheck" files="SendSE\.java" />
